{
  "compilerOptions": {
    "target": "es5",                         
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,                     
    "forceConsistentCasingInFileNames": true
  },
  "baseUrl": ".",
  "paths": {
    "@controllers/*": ["./src/controllers/*"],
    "@models/*": ["./src/models/*"],
    "@config/*": ["./src/config/*"]
  }
}
